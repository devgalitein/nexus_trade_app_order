<body>
  <div class="container-fluid">
    <div class="main-wraper">
      <div class="sidebar-main">
        <ul class="nav nav-pills sidebar-ul" id="menu">
          <li class="nav-item">
            <a href="/" class="nav-link align-middle px-0">
              <span class="fs-5 d-sm-inline">Menu</span>
            </a>
          </li>
          <li class="nav-item">
            <a href="/" class="nav-link align-middle px-0">
              <i class="fs-4 bi-house"></i>
              <span class="ms-1 d-sm-inline">Setting</span>
            </a>
          </li>

          <li class="nav-item">
            <a onclick="check()" class="nav-link px-0 align-middle" role="button">
              <i class="fs-4 bi-table"></i>
              <span class="ms-1 d-sm-inline ">Market live</span></a>
